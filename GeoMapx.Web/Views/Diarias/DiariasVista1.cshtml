<div class="col-md-6 heigth380">
    <div class="portlet box green">
        <div class="portlet-title">
            <div class="caption">
                <i class="fa fa-gift"></i>Planilla
            </div>
            <div class="tools">
                <a href="javascript:;" class="collapse"></a>
                <a href="javascript:;" class="fullscreen" data-original-title="" title=""></a>
                @*<a href="#portlet-config" data-toggle="modal" class="config"></a>*@
                <a href="javascript:;" class="reload"></a>
                <a href="javascript:;" class="remove"></a>
            </div>
            <div class="actions">
                <a data-ng-click="newEntrada()" data-ng-show="diarias.btns.Nuevo.show" class="btn default btn-sm">
                    <i class="fa fa-plus"></i> Nuevo
                </a>
                <a data-ng-click="cancelEntrada()" data-ng-show="diarias.btns.Cancelar.show" class="btn default btn-sm">
                    <i class="fa fa-undo"></i> Cancelar
                </a>
                <a data-ng-click="editarEntrada()" data-ng-show="diarias.btns.Editar.show"  class="btn default btn-sm">
                    <i class="fa fa-pencil"></i> Editar
                </a>
                <a data-ng-show="diarias.btns.Eliminar.show" class="btn red btn-sm">
    <i class="fa fa-eraser"></i> Eliminar
</a>@*class="btn btn-default btn-sm"*@
                <a data-ng-show="diarias.btns.Guardar.show" data-ng-click="guardarEntrada()" class="btn blue btn-sm">
                    <i class="fa fa-plus"></i> Guardar
                </a>
            </div>
        </div>
        <div class="portlet-body">
            <div class="form-group">
                <label class="col-md-3 control-label">Proyecto</label>
                <div class="col-md-9">
                    <select class="form-control" @*data-ng-change="getPostesByProyecto()"*@
                            data-ng-disabled="diarias.fieldsReadOnly"
                            @*data-ng-readonly="diarias.fieldsReadOnly"*@ data-ng-model="selectedProyect"
                            data-ng-options="proyecto.CodigoProyecto for proyecto in proyectosData"></select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Codigo Poste</label>
                <div class="col-md-9">
                    <select class="form-control"
                            data-ng-disabled="diarias.fieldsReadOnly"
                         data-ng-options="postes.CodigoPoste for postes in postesData" data-ng-model="selectedPoste">
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Unidad C.</label>
                <div class="col-md-9">
                    
                    <select class="form-control" data-ng-model="selectedUnicons"
                            data-ng-disabled="diarias.fieldsReadOnly"
                         data-ng-options="unicons.UniCons for unicons in uniconsData">
                    </select>
                </div>
                <span class="help-block">{{ selectedUnicons.DescripcionActividad }}</span>
            </div>
            <div class="form-group" data-ng-show="selectedUnicons.ActividadSecundaria.indexOf('TENDIDO')>=0">
                <label class="col-md-3 control-label">Hasta</label>                
                <div class="col-md-9">                        
                    <select class="form-control"
                            data-ng-disabled="diarias.fieldsReadOnly"
                         data-ng-options="postes.CodigoPoste for postes in postesHastaData " data-ng-model="selectedPosteHasta">
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Cantidad</label>
                <div class="col-md-9">
                    <input type="number" class="form-control" data-ng-model="cantidad"
                            data-ng-disabled="diarias.fieldsReadOnly"/>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Fecha</label>
                <div class="col-md-9">
                    <input type="date"  class="form-control" data-ng-model="fecha" data-ng-readonly="diarias.fieldsReadOnly" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Contratista</label>
                <div class="col-md-9">
                     <select class="form-control"
                            data-ng-disabled="diarias.fieldsReadOnly"
                         data-ng-options="lista.CodigoContratista for lista in contratistasData" data-ng-model="selectedContratista">
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Ficha</label>
                <div class="col-md-9">
                     <select class="form-control" data-ng-readonly="diarias.fieldsReadOnly"
                            data-ng-disabled="diarias.fieldsReadOnly"
                         data-ng-options="lista.CodigoFicha for lista in fichasData" data-ng-model="selectedFicha">
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Observación</label>
                <div class="col-md-9">
                    <input type="text" data-ng-readonly="diarias.fieldsReadOnly" class="form-control" data-ng-model="observacion" />
                </div>
            </div>

            <select style="visibility: hidden">
                <optgroup label="Group 1" class="group-1">
                    <option value="1-1">Option 1.1</option>
                    <option value="1-2" selected="selected">Option 1.2</option>
                    <option value="1-3" selected="selected">Option 1.3</option>
                </optgroup>
                <optgroup label="Group 2" class="group-2">
                    <option value="2-1">Option 2.1</option>
                    <option value="2-2">Option 2.2</option>
                    <option value="2-3">Option 2.3</option>
                </optgroup>
            </select>
        </div>
    </div>
</div>
