<div class="col-md-6 heigth380">
    <div class="portlet box green">
        <div class="portlet-title">
            <div class="caption">
                <i class="fa fa-gift"></i>Planilla
            </div>
            <div class="tools">
                <a href="javascript:;" class="collapse"></a>
                <a href="javascript:;" class="fullscreen" data-original-title="" title=""></a>
                @*<a href="#portlet-config" data-toggle="modal" class="config"></a>*@
                <a href="javascript:;" class="reload"></a>
                <a href="javascript:;" class="remove"></a>
            </div>
            <div class="actions">
                <a data-ng-click="newEntrada()" data-ng-show="diarias.btns.Nuevo.show" class="btn default btn-sm">
                    <i class="fa fa-plus"></i> Nuevo
                </a>
                <a data-ng-click="cancelEntrada()" data-ng-show="diarias.btns.Cancelar.show" class="btn default btn-sm">
                    <i class="fa fa-undo"></i> Cancelar
                </a>
                <a data-ng-click="editarEntrada()" data-ng-show="diarias.btns.Editar.show"  class="btn default btn-sm">
                    <i class="fa fa-pencil"></i> Editar
                </a>
                <a data-ng-show="diarias.btns.Eliminar.show" class="btn red btn-sm">
    <i class="fa fa-eraser"></i> Eliminar
</a>@*class="btn btn-default btn-sm"*@
                <a data-ng-show="diarias.btns.Guardar.show" data-ng-click="guardarEntrada()" class="btn blue btn-sm">
                    <i class="fa fa-plus"></i> Guardar
                </a>
            </div>
        </div>
        <div class="portlet-body">
            <div class="form-group">
                <label class="col-md-3 control-label">Proyecto</label>
                <div class="col-md-9">
                    <select class="form-control" 
                            data-ng-disabled="diarias.fieldsReadOnly"
                            data-ng-model="selectedProyect"
                            data-ng-options="proyecto.CodigoProyecto for proyecto in proyectosData"></select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Polígono</label>
                <div class="col-md-9">
                    <select class="form-control" 
                            data-ng-disabled="diarias.fieldsReadOnly"
                            data-ng-model="selectedPoligono"
                            data-ng-options="proyecto.CodigoPoligono for proyecto in poligonosData"></select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Codigo Poste</label>
                <div class="col-md-9">
                    <input type="text" data-ng-readonly="diarias.fieldsReadOnly" class="form-control" data-ng-model="model.CodigoPoste" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Tipo Poste</label>
                <div class="col-md-9">
                    <select class="form-control" 
                            data-ng-disabled="diarias.fieldsReadOnly"
                            data-ng-model="selectedTipoPoste"
                            data-ng-options="proyecto.TipoPosteDescripcion for proyecto in tiposPostesData"></select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Observación</label>
                <div class="col-md-9">
                    <input type="text" data-ng-readonly="diarias.fieldsReadOnly" class="form-control" data-ng-model="model.ObservacionPoste" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">X</label>
                <div class="col-md-9">
                    <input type="number" data-ng-readonly="diarias.fieldsReadOnly" class="form-control" data-ng-model="model.X" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Y</label>
                <div class="col-md-9">
                    <input type="number" data-ng-readonly="diarias.fieldsReadOnly" class="form-control" data-ng-model="model.Y" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Z</label>
                <div class="col-md-9">
                    <input type="number" data-ng-readonly="diarias.fieldsReadOnly" class="form-control" data-ng-model="model.Z" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Longitud</label>
                <div class="col-md-9">
                    <input type="number" data-ng-readonly="diarias.fieldsReadOnly" class="form-control" data-ng-model="model.Lon" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Latitud</label>
                <div class="col-md-9">
                    <input type="number" data-ng-readonly="diarias.fieldsReadOnly" class="form-control" data-ng-model="model.Lat" />
                </div>
            </div>

            <select style="visibility: hidden">
                <optgroup label="Group 1" class="group-1">
                    <option value="1-1">Option 1.1</option>
                    <option value="1-2" selected="selected">Option 1.2</option>
                    <option value="1-3" selected="selected">Option 1.3</option>
                </optgroup>
                <optgroup label="Group 2" class="group-2">
                    <option value="2-1">Option 2.1</option>
                    <option value="2-2">Option 2.2</option>
                    <option value="2-3">Option 2.3</option>
                </optgroup>
            </select>
        </div>
    </div>
</div>
