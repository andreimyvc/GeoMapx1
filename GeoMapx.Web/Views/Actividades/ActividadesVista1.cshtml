<div class="col-md-12">
    <div class="portlet box green">
        <div class="portlet-title">
            <div class="caption">
                Unidades Constructivas
            </div>
            <div class="tools">
                <a href="javascript:;" class="collapse"></a>
                <a href="javascript:;" class="fullscreen" data-original-title="" title=""></a>
                @*<a href="#portlet-config" data-toggle="modal" class="config"></a>*@
                <a href="javascript:;" class="reload"></a>
                <a href="javascript:;" class="remove"></a>
            </div>
            <div class="actions">
                <a data-ng-click="newEntrada()" data-ng-show="control.btns.Nuevo.show" class="btn default btn-sm">
                    <i class="fa fa-plus"></i> Nuevo
                </a>
                <a data-ng-click="cancelEntrada()" data-ng-show="control.btns.Cancelar.show" class="btn default btn-sm">
                    <i class="fa fa-undo"></i> Cancelar
                </a>
                <a data-ng-click="editarEntrada()" data-ng-show="control.btns.Editar.show"  class="btn default btn-sm">
                    <i class="fa fa-pencil"></i> Editar
                </a>
                <a data-ng-click="deleteEntrada()" data-ng-show="control.btns.Eliminar.show" class="btn red btn-sm">
    <i class="fa fa-eraser"></i> Eliminar
</a>@*class="btn btn-default btn-sm"*@
                <a data-ng-show="control.btns.Guardar.show" data-ng-click="guardarEntrada()" class="btn blue btn-sm">
                    <i class="fa fa-plus"></i> Guardar
                </a>
            </div>
        </div>
        <div class="portlet-body">
            <div class="form-group">
                <label class="col-md-3 control-label">Proyecto</label>
                <div class="col-md-9">
                    <select class="form-control" 
                            data-ng-disabled="control.fieldsReadOnly"  data-ng-model="selectedProyect"
                            data-ng-options="proyecto.CodigoProyecto for proyecto in proyectosData"></select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Unidad Constructiva</label>
                <div class="col-md-9">
                    <input type="text" data-ng-readonly="control.fieldsReadOnly" class="form-control" data-ng-model="newUnicons.UniCons" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Actividad Primaria</label>
                <div class="col-md-9">
                    <input type="text" data-ng-readonly="control.fieldsReadOnly" class="form-control" data-ng-model="newUnicons.ActividadPrimaria" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Actividad Secundaria</label>
                <div class="col-md-9">
                    <input type="text" data-ng-readonly="control.fieldsReadOnly" class="form-control" data-ng-model="newUnicons.ActividadSecundaria" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Actividad SGT</label>
                <div class="col-md-9">
                    <input type="text" data-ng-readonly="control.fieldsReadOnly" class="form-control" data-ng-model="newUnicons.ActividadSGT" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Cantidad</label>
                <div class="col-md-9">
                    <input type="number" data-ng-readonly="control.fieldsReadOnly" class="form-control" data-ng-model="newUnicons.Cantidad" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Precio Unitario</label>
                <div class="col-md-9">
                    <input type="number" data-ng-readonly="control.fieldsReadOnly" class="form-control" data-ng-model="newUnicons.PrecioUnitario" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Descripción</label>
                <div class="col-md-9">
                    <textarea   data-ng-readonly="control.fieldsReadOnly" class="form-control" data-ng-model="newUnicons.DescripcionActividad" ></textarea>
                </div>
            </div>
            <select style="visibility: hidden">
                <optgroup label="Group 1" class="group-1">
                    <option value="1-1">Option 1.1</option>
                    <option value="1-2" selected="selected">Option 1.2</option>
                    <option value="1-3" selected="selected">Option 1.3</option>
                </optgroup>
                <optgroup label="Group 2" class="group-2">
                    <option value="2-1">Option 2.1</option>
                    <option value="2-2">Option 2.2</option>
                    <option value="2-3">Option 2.3</option>
                </optgroup>
            </select>
        </div>
    </div>
</div>


